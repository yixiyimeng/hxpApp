<mescroll-body vue-id="1" down="{{downOption}}" up="{{upOption}}" data-event-opts="{{[['^down',[['downCallback']]],['^up',[['upCallback']]],['^init',[['mescrollInit']]]]}}" bind:down="__e" bind:up="__e" bind:init="__e" class="data-v-20cc408b" bind:__l="__l" vue-slots="{{['default']}}"><view class="pageview flex flex-direction data-v-20cc408b"><image class="bg data-v-20cc408b" src="/static/shop/balance_bg@2x.png" mode="widthFix"></image><view class="main data-v-20cc408b"><view class="walletinfo data-v-20cc408b"><view class="data-v-20cc408b">账户余额(元)</view><view class="price data-v-20cc408b">{{money}}</view><view class="tip data-v-20cc408b">{{"最多可提现 "+money+" 元，最小提现 10 元"}}</view></view><view class="flex applytxt align-center data-v-20cc408b"><label class="_span data-v-20cc408b">￥</label><input class="flex-sub data-v-20cc408b" type="digit" value="{{accountInfo.money}}" data-event-opts="{{[['input',[['__set_model',['$0','money','$event',[]],['accountInfo']],['oninput',['$event']]]]]}}" bindinput="__e"/><label data-event-opts="{{[['tap',[['getallMoney',['$event']]]]]}}" class="link _span data-v-20cc408b" bindtap="__e">全部提现</label></view><view class="applytip data-v-20cc408b">提现手续费：<text class="text-green margin-right data-v-20cc408b">{{"0.7%("+actualAmount+"元)"}}</text>最终到账金额：<text class="text-green data-v-20cc408b">{{txAmount+"元"}}</text></view><view class="accountlist flex justify-between data-v-20cc408b"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="{{['alipay flex flex-sub  align-center justify-center data-v-20cc408b',(curIndex==0)?'active':'']}}" bindtap="__e"><image src="/static/shop/alipay@2x.png" mode="widthFix" class="data-v-20cc408b"></image><label class="_span data-v-20cc408b">支付宝</label></view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="{{['flex wechat flex-sub align-center justify-center data-v-20cc408b',(curIndex==1)?'active':'']}}" bindtap="__e"><image src="/static/shop/wechat@2x.png" mode="widthFix" class="data-v-20cc408b"></image><label class="_span data-v-20cc408b">微信</label></view><view data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" class="{{['flex bank_card flex-sub align-center justify-center data-v-20cc408b',(curIndex==2)?'active':'']}}" bindtap="__e"><image src="/static/shop/bank_card@2x.png" mode="widthFix" class="data-v-20cc408b"></image><label class="_span data-v-20cc408b">银行卡</label></view></view><block wx:if="{{curIndex==0}}"><view class="applylist data-v-20cc408b"><view class="row data-v-20cc408b"><view class="data-v-20cc408b">支付宝账户：</view><input type="text" value="{{accountInfo.tx_number}}" placeholder="请输入支付宝账户" placeholder-style="color:#D2D2D2" data-event-opts="{{[['input',[['__set_model',['$0','tx_number','$event',[]],['accountInfo']]]]]}}" bindinput="__e" class="data-v-20cc408b"/></view><view class="row data-v-20cc408b"><view class="data-v-20cc408b">支付宝真实姓名：</view><input type="text" value="{{accountInfo.tx_name}}" placeholder="请输入支付宝账户" placeholder-style="color:#D2D2D2" data-event-opts="{{[['input',[['__set_model',['$0','tx_name','$event',[]],['accountInfo']]]]]}}" bindinput="__e" class="data-v-20cc408b"/></view></view></block><block wx:if="{{curIndex==1}}"><view class="applylist data-v-20cc408b"><view class="row data-v-20cc408b"><view class="data-v-20cc408b">微信账户：</view><input type="text" value="{{accountInfo.tx_number}}" placeholder="请输入微信账户" placeholder-style="color:#D2D2D2" data-event-opts="{{[['input',[['__set_model',['$0','tx_number','$event',[]],['accountInfo']]]]]}}" bindinput="__e" class="data-v-20cc408b"/></view></view></block><block wx:if="{{curIndex==2}}"><view class="applylist data-v-20cc408b"><view class="row data-v-20cc408b"><view class="data-v-20cc408b">银行卡账户：</view><input type="text" value="{{accountInfo.tx_number}}" placeholder="请输入银行卡账户" placeholder-style="color:#D2D2D2" data-event-opts="{{[['input',[['__set_model',['$0','tx_number','$event',[]],['accountInfo']]]]]}}" bindinput="__e" class="data-v-20cc408b"/></view><view class="row data-v-20cc408b"><view class="data-v-20cc408b">开户银行：</view><input type="text" value="{{accountInfo.tx_address}}" placeholder="请输入开户银行" placeholder-style="color:#D2D2D2" data-event-opts="{{[['input',[['__set_model',['$0','tx_address','$event',[]],['accountInfo']]]]]}}" bindinput="__e" class="data-v-20cc408b"/></view><view class="row data-v-20cc408b"><view class="data-v-20cc408b">开户姓名：</view><input type="text" value="{{accountInfo.tx_name}}" placeholder="请输入开户姓名" placeholder-style="color:#D2D2D2" data-event-opts="{{[['input',[['__set_model',['$0','tx_name','$event',[]],['accountInfo']]]]]}}" bindinput="__e" class="data-v-20cc408b"/></view></view></block><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="btn data-v-20cc408b" bindtap="__e">确认提现</view></view></view></mescroll-body>