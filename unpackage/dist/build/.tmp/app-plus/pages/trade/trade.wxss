    color: #2F2F2F;
    font-size: 30rpx;
}
.filterbar .inputbox {
    height: 70rpx;
    line-height: 70rpx;
    border: 1px solid #9D9D9D;
    border-radius: 8rpx;
    padding: 0 18rpx;
}
.filterbar .inputbox image {
